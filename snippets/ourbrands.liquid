<ul id="brandlist">
<a href="/brands/all" title="Browse All" class="shopall allbrands block">#</a>
{% for collection in collections %}{% unless collection.handle == 'frontpage' or collection.handle == 'new-arrivals' or collection.handle == 'sale' or collection.metafields.c_f.nav_brand_selector == 'hide'%}{% capture collection_title %}{{ collection.title | escape }}{% endcapture %}{% if collection.products_count > 0 %}<li class="brandlink">

			<p>{{collection.handel}}
			<a href="{{ collection.url }}" title="Browse {{ collection_title }}">{{ collection.title }}</a></p>
		</li>{% endif %}{% cycle '', '', new_row %}{% endunless %}{% endfor %}
</ul>